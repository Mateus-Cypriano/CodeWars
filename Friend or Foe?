function friend(friends){
  const new_friends = friends.filter((name) => name.length == 4);
  return new_friends;
  //your code here
}

console.log(friend(['Ryan', 'Kieran', 'Mark']));
console.log(friend(['Ryan', 'Jimmy', '123', 'Cool Man']));
console.log(friend(['Jimm', 'Cari', 'aret', 'truehaskskj', 'sixtyiscooooool']));
console.log(friend(['Love', 'Your', 'Face', '1']));
